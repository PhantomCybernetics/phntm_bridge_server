{
  "name": "phntm_bridge_server",
  "version": "0.0.1",
  "description": "Signalling server for Phantom Bridge",
  "packageManager": "bun@1.2.10",
  "scripts": {
    "build": "tsc --pretty --allowUnreachableCode -p ./tsconfig/bridge.json",
    "start-file-receiver": "bun src/fileReceiver.ts",
    "start-bridge": "node ./build/CloudBridge.js",
    "dev-bridge": "tsx --watch ./src/CloudBridge.js",
    "dev-file-receiver": "bun --watch src/fileReceiver.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PhantomCybernetics/phntm_bridge_server.git"
  },
  "keywords": [
    "ros",
    "ros2",
    "phntm",
    "phantom",
    "bridge",
    "webrtc",
    "socket.io",
    "robot",
    "video",
    "streaming",
    "teleoperation",
    "remote",
    "operation"
  ],
  "author": "Mirek Burkon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PhantomCybernetics/phntm_bridge_server/issues"
  },
  "homepage": "https://github.com/PhantomCybernetics/phntm_bridge_server#readme",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.723.0",
    "@foxglove/message-definition": "^0.3.0",
    "@foxglove/rosmsg": "^4.2.2",
    "axios": "^1.7.9",
    "bcrypt-nodejs": "^0.0.3",
    "browserify": "^17.0.0",
    "colors": "^1.4.0",
    "comment-json": "^4.2.3",
    "ejs": "^3.1.10",
    "engine.io": "^6.6.4",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "gridstack": "^9.4.0",
    "js-yaml": "^4.1.0",
    "mongodb": "^5.5.0",
    "multer": "^1.4.5-lts.1",
    "nearley": "^2.20.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.1",
    "three": "^0.157.0",
    "tsx": "^4.20.3",
    "xml-formatter": "^3.4.1"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.23",
    "@types/js-yaml": "^4.0.5",
    "@types/nearley": "^2.11.2",
    "@types/node": "^18.19.115",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
